    <name>ws-plugin-vue-runner</name>
    <vendor>qingying233</vendor>

    <depends>com.intellij.modules.platform</depends>

    <resource-bundle>messages.MyBundle</resource-bundle>

    <extensions defaultExtensionNs="com.intellij">
        <toolWindow factoryClass="com.github.qingying233.wspluginvuerunner.toolWindow.MyToolWindowFactory" id="MyToolWindow"/>
